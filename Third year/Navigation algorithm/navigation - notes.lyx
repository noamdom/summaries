\begin_layout Itemize
משוואות התנועה המוכללות 
\end_layout

\begin_deeper
\begin_layout Itemize
אם 
\begin_inset Formula $\theta=0$
\end_inset

 אז:
\end_layout

\begin_deeper
\begin_layout Standard
\begin_inset Formula 
\[
\begin{matrix}x_{f}=x_{i}+v\cdot\Delta t\times\cos\left(\theta_{0}\right)\\
y_{f}=y_{i}+v\cdot\Delta t\times sin\left(\theta_{0}\right)
\end{matrix}
\]

\end_inset


\end_layout

\end_deeper
\begin_layout Itemize
אם 
\begin_inset Formula $\theta\neq0$
\end_inset

 אז:
\end_layout

\begin_deeper
\begin_layout Standard
\begin_inset Formula 
\[
\begin{matrix}x_{f}=x_{i}+\frac{v}{\dot{\theta}}\left(\sin\left(\theta_{0}+\Delta t\times\dot{\theta}\right)-\sin\left(\theta_{0}\right)\right)\\
y_{f}=y_{i}+\frac{v}{\dot{\theta}}\left(\cos\left(\theta_{0}\right)-\cos\left(\theta_{0}+\Delta t\times\dot{\theta}\right)\right)\\
\theta_{f}=\theta_{0}+\Delta t\times\dot{\theta}
\end{matrix}
\]

\end_inset


\end_layout

\end_deeper
\end_deeper
\begin_layout Itemize
התמרות הומגניות 
\begin_inset Formula $\text{rotation matrix }$
\end_inset

 :
\end_layout

\begin_deeper
\begin_layout Standard
\align center
\begin_inset Formula 
\[
\left[\begin{matrix}x_{m}\\
y_{m}\\
1
\end{matrix}\right]=\left[\begin{matrix}\cos\theta & -\sin\theta & x_{p}\\
\sin\theta & \cos\theta & x_{p}\\
0 & 0 & 1
\end{matrix}\right]\left[\begin{matrix}x_{c}\\
y_{c}\\
1
\end{matrix}\right]
\]

\end_inset


\end_layout

\end_deeper
